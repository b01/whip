{
    "authors": [
        {
            "homepage": "http://www.kshabazz.net/",
            "name": "Khalifah Khalil Shabazz",
            "role": "Developer"
        }

    ],
    "autoload": {
        "psr-4": {
            "Whip\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Whip\\Tests\\": "test/Whip"
        }
    },
    "description": "Whip web tools.",
    "license": "MIT",
    "name": "kshabazz/whip",
    "require": {
        "kshabazz/slib": "dev-master",
        "php": ">=7.0",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0",
        "whip/lash": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^2.0",
        "phpmd/phpmd": "*",
        "twig/twig": "^2.0"
    },
    "scripts": {
        "units": "phpunit",
        "pcs": "./vendor/bin/phpcs -p --standard=PSR2 --severity=8 --colors src"
    },
    "suggest": {
        "twig/twig": "Required if you plan to use the TwigRendered."
    }
}
